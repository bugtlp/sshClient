{
  "name": "sshclient",
  "private": true,
  "version": "1.0.0",
  "description": "SSH client",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "precommit": "npm run lint",
    "prepush": "npm run lint && npm test unit"
  },
  "keywords": [
    "ssh"
  ],
  "author": "Kireev Anatoly",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^8.10.38",
    "@types/ssh2": "0.5.37",
    "husky": "^1.3.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": ">=8.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ]
  },
  "dependencies": {
    "commander": "^2.19.0",
    "ssh2": "0.6.1"
  }
}
